{
  "name": "dnd-encounters",
  "version": "1.0.0",
  "description": "rpg",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server.js",
    "dnd": "npm start --prefix dnd",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "npm start",
    "start:dev": "concurrently \"npm run watch\" \"cd dnd && npm start\"",
    "install": "npm i && cd ../dnd && npm i",
    "seed": "npm run seed",
    "heroku-postbuild": "cd dnd && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zapblumar/DND-encounters.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zapblumar/DND-encounters/issues"
  },
  "homepage": "https://github.com/Zapblumar/DND-encounters#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "mongo": "^0.1.0",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "router": "^1.3.5",
    "socket.io": "^3.1.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
